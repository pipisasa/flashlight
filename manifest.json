{
	"manifest_version": 2,
	"name": "Tproger Launcher",
	"description": "Запускатор представительств Tproger",
	"version": "1.0.0",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": [ "*://*/*" ],
			"js": [ "a/content.js" ]
		}
	],
	"permissions": ["activeTab"]
}